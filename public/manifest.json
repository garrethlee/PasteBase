{
  "name": "PasteBase",
  "description": "Get your frequent copy-pastes in one nifty location!",
  "version": "0.0.0.1",
  "manifest_version":3,
  "action": {
    "default_popup":"index.html",
    "default_title":"Open PasteBase",
    "default_icon": { 
      "16":"assets/16.png",
      "32":"assets/32.png",
      "64":"assets/64.png",
      "128":"assets/128.png"
    },
    "permissions":["scripting","notifications"]
  }
}